{
  "name": "debian-image-builder",
  "version": "1.0.1",
  "description": "build Debian disk images (with Docker)",
  "bin": "./build",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "example:leveldown:arm": "./build --docker --arch arm --input examples/leveldown --output output",
    "example:leveldown:arm64": "./build --docker --arch arm64 --input examples/leveldown --output output",
    "example:sodium-native:arm": "./build --docker --arch arm --input examples/sodium-native --output output",
    "example:sodium-native:arm64": "./build --docker --arch arm64 --input examples/sodium-native --output output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ahdinosaur/debian-image-builder.git"
  },
  "keywords": [
    "debian",
    "vmdb2",
    "vmdebootrap"
  ],
  "author": "ahdinosaur",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/debian-image-builder/issues"
  },
  "homepage": "https://github.com/ahdinosaur/debian-image-builder#readme"
}
